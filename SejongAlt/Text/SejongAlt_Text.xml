    <!-- 한국어 -->
    <Replace Tag="LOC_PEDIA_LEADERS_PAGE_LEADER_SEJONG_TITLE" Language="ko_KR">
      <Text>세종대왕(깨우친 임금)</Text>
    </Replace>
    <Replace Tag="LOC_PEDIA_LEADERS_PAGE_LEADER_SEJONG_ALT_TITLE" Language="ko_KR">
